{
  "private": true,
  "scripts": {
    "start": "cross-env NODE_OPTIONS=--openssl-legacy-provider PORT=8888 umi dev",
    "build": "NODE_OPTIONS=--openssl-legacy-provider umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "5.0.12",
    "@azure/msal-browser": "^2.16.0",
    "@azure/msal-react": "^1.0.1",
    "@fullcalendar/core": "5.9.0",
    "@fullcalendar/daygrid": "5.9.0",
    "@fullcalendar/interaction": "5.9.0",
    "@fullcalendar/react": "5.9.0",
    "@fullcalendar/timegrid": "5.9.0",
    "@react-oauth/google": "^0.12.1",
    "@stripe/react-stripe-js": "^1.1.2",
    "@stripe/stripe-js": "^1.11.0",
    "@umijs/preset-react": "1.8.2",
    "antd-img-crop": "3.13.2",
    "cookie": "^0.4.1",
    "dayjs": "^1.9.6",
    "immutability-helper": "^3.1.1",
    "js-cookie": "^2.2.1",
    "moment-timezone": "^0.5.31",
    "react": "16.12.0",
    "react-csv": "^2.0.3",
    "react-dom": "16.12.0",
    "react-ga": "^3.3.0",
    "react-google-login": "^5.1.22",
    "react-gtm-module": "^2.0.11",
    "react-jquery-plugin": "^1.1.7",
    "react-loading": "^2.0.3",
    "react-microsoft-login": "^1.10.1",
    "react-swipe-to-delete-ios": "^2.1.0",
    "react-swipeable": "^7.0.1",
    "react-swipeable-list": "^1.9.3",
    "react-ui-animate": "1.0.3",
    "reactjs-popup": "^2.0.5",
    "socket.io-client": "^2.4.0",
    "superagent": "^6.1.0",
    "umi": "3.2.23",
    "uuid": "^3.4.0"
  },
  "devDependencies": {
    "@umijs/test": "3.2.23",
    "cross-env": "^10.0.0",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "yorkie": "^2.0.0"
  }
